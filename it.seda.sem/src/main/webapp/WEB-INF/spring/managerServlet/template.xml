<templates>
	<template name="default" url="/template/manager/default.jsp" default="true"/>
	<template name="alternate" url="/template/manager/default.jsp"/>	

	<screen name="base">
		<parameters>
			<parameter key="title" value="manager.title"/>
			<parameter key="header" value="/template/manager/header.jsp"/>
			<parameter key="menu" value="/template/manager/menu.jsp"/>
			<parameter key="footer" value="/template/manager/footer.jsp"/>			
		</parameters>
	</screen>
	
	<screen name="formClient" template="alternate" extends="base">
		<parameters>	
			<parameter key="title" value="manager.formclient.title"/>	
			<parameter key="body" value="/template/manager/pages/formClient.jsp"/>
		</parameters>
	
	</screen>

</templates>
